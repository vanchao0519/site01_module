{# ** @file modules/custom/site01/templates/site01_entity.html.twig
 *
 * Default theme implementation to present Artwork data.
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 *          content.field_name to access public fields
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_site01_entity()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('site01_entity') }}>
